{
  "name": "ScriptHero",
  "version": "1.0.0",
  "description": "Trello plugin to write scripts",
  "main": "app.js",
  "bin": "app.js",
  "pkg": {
    "assets": [
      "src/client/public/*",
      "src/client/index.html"
    ],
    "targets": [
      "node9-linux",
      "node9-macos",
      "node9-win-x64"
    ]
  },
  "scripts": {
    "start": "node app",
    "watch": "webpack --watch & webpack-dev-server",
    "build": "webpack --progress --profile --colors",
    "clean": "rm -rf ./clien/public"
  },
  "author": "Jegor van Opdorp",
  "license": "Commercial",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-textarea-autosize": "^5.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0"
  },
  "engines": {
    "node": "9.6.0"
  }
}
